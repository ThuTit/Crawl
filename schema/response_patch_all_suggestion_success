{
  "definitions": {},
  "$schema": "",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "code",
    "result",
    "message"
  ],
  "properties": {
    "code": {
      "$id": "#/properties/code",
      "type": "string",
      "title": "The Code Schema"
    },
    "result": {
      "$id": "#/properties/result",
      "type": "object",
      "title": "The Result Schema"
    },
    "message": {
      "$id": "#/properties/message",
      "type": "string",
      "title": "The Message Schema"
    }
  }
}